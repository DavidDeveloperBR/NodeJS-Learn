<h3>Consultas</h3>
    <form action="/searchresult" method="get">
        <input type="search" name="search" id="search" placeholder="Digite o e-mail ou cpf do cliente">
        <button>Pesquisar</button>
    </form>
<hr>
<% appos.forEach(appo =>{ %> 
    <h4>Consulta: <%=appo.description%></h4>
    <h4>Paciente: <%=appo.name %></h4>
    <h4>E-mail: <%=appo.email %></h4>
    <h4>CPF: <%=appo.cpf %></h4>
    <hr>
<% }) %>